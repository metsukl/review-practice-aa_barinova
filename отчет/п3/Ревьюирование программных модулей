import tkinter as tk
   import webbrowser

   def open_url():
       url = https://docs.google.com/document/d/1jvvljLB0GuB0IoUO62Tw4tR2Z4B6knZFJkiAkCnBljY/edit?usp=sharing

       webbrowser.open(url)

   root = tk.Tk()
   root.title("Ссылка в GUI")

   link_text = "тык"
   link_label = tk.Label(root, text=link_text, fg="blue", cursor="hand2")  # "hand2" - курсор руки
   link_label.bind("<Button-1>", open_url) 
   link_label.pack()

   root.mainloop()

